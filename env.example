# Environment Variables for LiquidAI
# Copy this file to .env and fill in your values

# Ethereum RPC URLs
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# Private Key (for deployment)
PRIVATE_KEY=your_private_key_here

# Pyth Network
PYTH_PRICE_SERVICE_URL=https://hermes.pyth.network
PYTH_CONTRACT_ADDRESS=0x... # Pyth contract address on your network

# Envio Indexer
ENVIO_API_URL=https://api.envio.dev
ENVIO_API_KEY=your_envio_api_key

# ASI:One Integration
ASI_API_KEY=your_asi_api_key
ASI_AGENT_ID=your_agent_id

# MeTTa Configuration
METTA_ENDPOINT=https://api.metta.ai
METTA_API_KEY=your_metta_api_key
